<div id="{{get-section-id layout.title}}" class="p-xl" style="{{containerStyle}}">
    <div class="container">
        <div class="row text-center">
            <h2 style="display: inline">
                {{layout.title}}
                <a class="headerlink" href="#{{get-section-id layout.title}}">
                    <i class="fa fa-link"></i>
                </a>
            </h2>
        </div>
        <div class="row">
            {{#each data as |categorySponsors|}}
                <div class="row">
                {{#if categorySponsors.category}}
                    <h3>{{categorySponsors.category}}</h3><hr>
                {{/if}}
                {{#each categorySponsors.sponsors as |sponsor|}}
                    <div class="text-center col-xl-1 col-lg-3 col-md-3 col-sm-4 col-xs-12">
                        <div class="logo-wrapper">
                            <a href="{{sponsor.website_url}}">
                                <img class="sponsors-logo m-b-md" src="{{sponsor.img_url}}"/>
                            </a>
                        </div>
                    </div>
                {{/each}}
                </div>
            {{/each}}
        </div>
    </div>
</div>
